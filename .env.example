# Thin-Waist API 的基础地址（默认 http://localhost:8700/api/v1）
SOULSEED_API_BASE_URL=http://localhost:8700/api/v1

# SSE 流的独立地址（可选，不填时复用 API 地址）
SOULSEED_STREAM_BASE_URL=http://localhost:8700/api/v1

# 默认绑定的租户、会话 ID（可选，用于自动填充控制台）
SOULSEED_DEFAULT_TENANT=tenant_alpha
SOULSEED_DEFAULT_SESSION=session_alpha

# 访问后端所需的 Bearer Token（可选，最终会被拼成 Authorization: Bearer <token>）
SOULSEED_AUTH_TOKEN=demo-token

# 运行模式：dev 或 prod（默认 dev）
SOULSEED_PROFILE=dev

# SSE 心跳超时（毫秒，可选，默认 30000）
SOULSEED_SSE_TIMEOUT_MS=30000

# HTTP 请求超时（秒，可选，默认 15）
SOULSEED_REQUEST_TIMEOUT_SECS=15
